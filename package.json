{
  "name": "starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check-format": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "bundle": "webpack -p",
    "start": "webpack-dev-server",
    "ci": "npm run check-format && npm run bundle && npm test",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!src/**/*.d.ts"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^1.23.1",
    "copy-to-clipboard": "^3.0.7",
    "csstips": "^0.2.0",
    "csx": "^8.2.1",
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "mobx": "^3.1.16",
    "mobx-react": "^4.2.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.5.4",
    "react-json-view": "^1.10.12",
    "react-transition-group": "^2.2.0",
    "typestyle": "^1.3.7",
    "victory": "^0.21.2"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/jest": "^19.2.3",
    "@types/lodash": "^4.14.68",
    "@types/node": "^7.0.33",
    "@types/prop-types": "^15.5.1",
    "@types/pure-render-decorator": "^0.2.27",
    "@types/react": "^15.0.39",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-dom": "^15.5.1",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/victory": "^0.9.10",
    "@types/webpack": "^3.0.4",
    "awesome-typescript-loader": "^3.2.1",
    "coveralls": "^2.13.1",
    "fork-ts-checker-notifier-webpack-plugin": "^0.2.0",
    "fork-ts-checker-webpack-plugin": "^0.2.7",
    "jest": "^20.0.3",
    "prettier": "^1.5.3",
    "ts-jest": "^20.0.4",
    "ts-loader": "^2.3.0",
    "ts-node": "^3.2.0",
    "typescript": "^2.3.3",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  }
}
